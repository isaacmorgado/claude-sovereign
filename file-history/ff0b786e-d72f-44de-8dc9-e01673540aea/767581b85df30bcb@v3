  createFaceIQScoreGradient,
  getFaceIQDeviationDescription,
  getFaceIQSeverity,
  getObfuscatedSliderPosition,
  FACEIQ_COLOR_STOPS,
  type FaceIQScoringConfig,
  type FaceIQCurvePoint,
  type FaceIQScoreResult,
} from './scoring';

// Metric-to-Landmark Mappings
export {
  FACEIQ_FRONT_METRIC_LANDMARKS,
  FACEIQ_SIDE_METRIC_LANDMARKS,
  getFaceIQMetricLandmarks,
  getMetricsUsingLandmark,
  getAllUsedLandmarks,
  FRONT_METRIC_COUNT,
