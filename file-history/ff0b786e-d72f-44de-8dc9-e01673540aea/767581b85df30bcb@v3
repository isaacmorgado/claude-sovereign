/**
 * FaceIQ Parity Module
 *
 * This module provides 100% parity with FaceIQ's facial analysis system.
 * All algorithms, formulas, and mappings have been reverse-engineered from
 * FaceIQ's production JavaScript to ensure identical results.
 *
 * Features:
 * - 31 side profile landmarks matching FaceIQ's exact order and definitions
 * - Exponential decay scoring with FaceIQ's exact formula
 * - FaceIQ color gradient for score visualization
 * - Metric-to-landmark mappings for both front and side views
 * - FaceIQ-style rotation normalization using indices 10, 26, 75
 */

// Side Profile Landmarks
export {
  FACEIQ_SIDE_LANDMARKS,
  getSideLandmarkById,
  getSideLandmarksByCategory,
  getSideLandmarkIndex,
  SIDE_LANDMARK_COUNT,
  SIDE_LANDMARK_IDS,
  SIDE_LANDMARK_ZOOM_LEVELS,
  type SideLandmarkDefinition,
} from './sideLandmarks';

// Scoring Algorithm
export {
  calculateFaceIQScore,
  interpolateFaceIQCurve,
  getFaceIQScoreColor,
