import { getMetricConfigForDemographics } from './src/lib/faceiq-scoring';

console.log('\n=== Female Demographic Metrics Verification ===\n');

// Test White Female FWHR
const whiteFemale = getMetricConfigForDemographics('faceWidthToHeight', 'female', 'white');
console.log('1. White Female FWHR:', whiteFemale?.idealMin, '-', whiteFemale?.idealMax);
console.log('   Expected: 1.45 - 1.53');
console.log('   ✓ Match:', whiteFemale?.idealMin === 1.45 && whiteFemale?.idealMax === 1.53);

// Test Hispanic Female Lip Ratio
const hispanicFemale = getMetricConfigForDemographics('lipRatio', 'female', 'hispanic');
console.log('\n2. Hispanic Female Lip Ratio:', hispanicFemale?.idealMin, '-', hispanicFemale?.idealMax);
console.log('   Expected: 1.1 - 1.35');
console.log('   ✓ Match:', hispanicFemale?.idealMin === 1.1 && hispanicFemale?.idealMax === 1.35);

// Test Pacific Islander Female FWHR
const pacificFemale = getMetricConfigForDemographics('faceWidthToHeight', 'female', 'pacific_islander');
console.log('\n3. Pacific Islander Female FWHR:', pacificFemale?.idealMin, '-', pacificFemale?.idealMax);
console.log('   Expected: 1.55 - 1.70');
console.log('   ✓ Match:', pacificFemale?.idealMin === 1.55 && pacificFemale?.idealMax === 1.70);

// Test Native American Female Cheekbone Height
const nativeFemale = getMetricConfigForDemographics('cheekboneHeight', 'female', 'native_american');
console.log('\n4. Native American Female Cheekbone Height:', nativeFemale?.idealMin, '-', nativeFemale?.idealMax);
console.log('   Expected: 1.5 - 2.5');
console.log('   ✓ Match:', nativeFemale?.idealMin === 1.5 && nativeFemale?.idealMax === 2.5);

// Test Black Female Lip Ratio
const blackFemale = getMetricConfigForDemographics('lipRatio', 'female', 'black');
console.log('\n5. Black Female Lip Ratio:', blackFemale?.idealMin, '-', blackFemale?.idealMax);
console.log('   Expected: 1.3 - 1.6');
console.log('   ✓ Match:', blackFemale?.idealMin === 1.3 && blackFemale?.idealMax === 1.6);

// Test East Asian Female Eye Separation
const eastAsianFemale = getMetricConfigForDemographics('eyeSeparationRatio', 'female', 'east_asian');
console.log('\n6. East Asian Female Eye Separation:', eastAsianFemale?.idealMin, '-', eastAsianFemale?.idealMax);
console.log('   Expected: 46.3 - 47.5');
console.log('   ✓ Match:', eastAsianFemale?.idealMin === 46.3 && eastAsianFemale?.idealMax === 47.5);

// Test Middle Eastern Female Eyebrow Thickness
const middleEasternFemale = getMetricConfigForDemographics('eyebrowThickness', 'female', 'middle_eastern');
console.log('\n7. Middle Eastern Female Eyebrow Thickness:', middleEasternFemale?.idealMin, '-', middleEasternFemale?.idealMax);
console.log('   Expected: 1.5 - 3.0');
console.log('   ✓ Match:', middleEasternFemale?.idealMin === 1.5 && middleEasternFemale?.idealMax === 3.0);

// Test South Asian Female Tear Trough Depth
const southAsianFemale = getMetricConfigForDemographics('tearTroughDepth', 'female', 'south_asian');
console.log('\n8. South Asian Female Tear Trough Depth:', southAsianFemale?.idealMin, '-', southAsianFemale?.idealMax);
console.log('   Expected: 0.0 - 0.5');
console.log('   ✓ Match:', southAsianFemale?.idealMin === 0.0 && southAsianFemale?.idealMax === 0.5);

console.log('\n=== All 8 Female Demographics Verified ✅ ===\n');
