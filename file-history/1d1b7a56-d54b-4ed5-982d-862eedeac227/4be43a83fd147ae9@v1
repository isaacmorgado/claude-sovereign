import { startTranscriptionWorker } from './transcription-worker.js'
import { startVocalIsolationWorker } from './vocal-isolation-worker.js'

export function startAllWorkers(): void {
  console.log('Starting all workers...')

  startTranscriptionWorker()
  startVocalIsolationWorker()

  console.log('All workers started')
}

export { startTranscriptionWorker, startVocalIsolationWorker }
